#include <iostream>

using namespace std;

int f(int x){
if (x==1||x==2) return 1;
for (int i=2; i<=x/2;i+=2){

    if (x%i==0) return 0;
if (i==2) i++;
}
return 1;
}

int main()
{   int a=0;
int   mas =0;
    int count =0;
    int max =0;
    int arr[100000];

    for (int k=0; k<200000;k++){
        if (f(k)) { arr[a]=k; a++; mas++;}
    }

   for (int i=125697; i<=190234; i++){
        for ( int j = 0; j<mas; j++){
            if (i%arr[j]==0&&f(i/arr[j])) {count++; max=i;  break;}
        }
    }
    cout <<count<<' '<< max;

    return 0;
}
