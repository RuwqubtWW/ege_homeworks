#include <iostream>
#include <fstream>
#include <string>
#include <vector>
#include <algorithm>
#include <cmath>
using namespace std;


int main(){

int sqrti;
int count=0;
int max=0;
for (int i=101000000; i<=102000000; i++){
    sqrti= sqrt(i);
    for (int j=1; j<=round(sqrti); j++){

        if(i%j==0){
          if(j%2==0) count++;
          if ((i/j)%2==0&&j!=i/j)  count++;
        }
    }
    if (count>max) max=count;
    count=0;
}
cout <<max;
return 0;
}
